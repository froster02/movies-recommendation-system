   "source": [
    "import numpy as np\n",
    "\n",
    "# consider two vectors A and B in 2-D\n",
    "A=np.array([7,3])\n",
    "B=np.array([3,7])\n",
    "\n",
    "# cosine similarity between A and B\n",
    "cos_sim=np.dot(A,B)/(np.linalg.norm(A)*np.linalg.norm(B))\n",
    "print (f\"Cosine Similarity : {cos_sim}\")\n",
    "print (f\"Cosine Distance   : {1-cos_sim}\")\n"
   ]
